<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <WaspAdditionalIncludeDirectories>$(SolutionDir)include;$(SolutionDir)Utils;$(SolutionDir)Token;$(SolutionDir)SemanticAnalyzer;$(SolutionDir)Parser;$(SolutionDir)MemorySystem;$(SolutionDir)Lexer;$(SolutionDir)Compiler;$(SolutionDir)AST;$(SolutionDir)AnnotatedAST;$(SolutionDir)ObjectSystem;$(SolutionDir)VM;$(SolutionDir)Symbol;$(SolutionDir)TypeSystem;</WaspAdditionalIncludeDirectories>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="WaspAdditionalIncludeDirectories">
      <Value>$(WaspAdditionalIncludeDirectories)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>